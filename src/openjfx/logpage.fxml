<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.geometry.*?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>


<BorderPane fx:id="logPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" 
	
	xmlns="http://javafx.com/javafx/8" 
	xmlns:fx="http://javafx.com/fxml/1" 
	
	fx:controller="openjfx.LogTabController">
	
	<top>
		<Label fx:id="logLabel" text="Days with valid samples:" 
			style=" -fx-border-color:grey; -fx-border-width: 0; -fx-border-style: solid;"/>
	</top>
	
	<center>
		<TextArea fx:id="logTextArea" text="(Process files to see log)" prefHeight="500.0" prefWidth="800.0"/>
	</center>
	
</BorderPane>